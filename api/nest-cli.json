{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "channel": {
      "type": "library",
      "root": "libs/channel",
      "entryFile": "index",
      "sourceRoot": "libs/channel/src",
      "compilerOptions": {
        "tsConfigPath": "libs/channel/tsconfig.lib.json"
      }
    },
    "request-context": {
      "type": "library",
      "root": "libs/request-context",
      "entryFile": "index",
      "sourceRoot": "libs/request-context/src",
